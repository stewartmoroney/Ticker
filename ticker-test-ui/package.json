  "dependencies": {
    "@react-rxjs/core": "^0.2.0",
    "@types/jest": "^24.0.2",
    "@types/node": "^12.12.47",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/rx": "^4.1.2",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^3.4.5",
    "cross-env": "^5.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.7.0",
    "react-scripts": "3.4.1",
