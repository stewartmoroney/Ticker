    "mock-socket": "^9.0.3",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint -c ./.eslintrc-format.json --fix",
      "git add"
    ],
    "*.+(js|jsx|json|md)": [
      "prettier --write",
      "git add"
    ]
  }
